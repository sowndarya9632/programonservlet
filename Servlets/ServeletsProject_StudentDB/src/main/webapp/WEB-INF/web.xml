<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  <servlet-name>whatever</servlet-name>
  <servlet-class>com.jsp.servelet.dynamic_app.DemoServelet1</servlet-class>
  </servlet>
  
  <!-- servlet Tags -->
  <servlet>
  <servlet-name>ADD</servlet-name>
  <servlet-class>com.jsp.servelet.employeedb.AddEmployeeServlet</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>update</servlet-name>
  <servlet-class>com.jsp.servelet.employeedb.UpdateEmployeeServlet</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>delete</servlet-name>
  <servlet-class>com.jsp.servelet.employeedb.DeleteEmployeeServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>whatever</servlet-name> 
  <url-pattern>/anything</url-pattern>
  </servlet-mapping>
  
  <!-- servlet Mapping Tags -->
  <servlet-mapping>
  <servlet-name>ADD</servlet-name> 
  <url-pattern>/Add-Employee</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>update</servlet-name> 
  <url-pattern>/Update-Employee</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>delete</servlet-name> 
  <url-pattern>/Delete-Employee</url-pattern>
  </servlet-mapping>
  
</web-app>
