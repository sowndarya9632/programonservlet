<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<!-- Servlet Tags -->
	<servlet>
		<servlet-name>ADD</servlet-name>
		<servlet-class>com.jsp.servlet.calculator.AddNumbersServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SUB</servlet-name>
		<servlet-class>com.jsp.servlet.calculator.SubstractNumberServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MUL</servlet-name>
		<servlet-class>com.jsp.servlet.calculator.MultiplyNumbersServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DIV</servlet-name>
		<servlet-class>com.jsp.servlet.calculator.DivisionNumberServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MOD</servlet-name>
		<servlet-class>com.jsp.servlet.calculator.ModulusNumbersServlet</servlet-class>
	</servlet>



	<!-- Servlet-Mapping Tags -->
	<servlet-mapping>
		<servlet-name>ADD</servlet-name>
		<url-pattern>/ADD</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>SUB</servlet-name>
		<url-pattern>/SUB</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>MUL</servlet-name>
		<url-pattern>/MUL</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>DIV</servlet-name>
		<url-pattern>/DIV</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>MOD</servlet-name>
		<url-pattern>/MOD</url-pattern>
	</servlet-mapping>

</web-app>
